
public class Swaping {
    public static void main(String[] args) {
        int x = 10;
        int y = 20;
        
        System.out.println("Before swapping x and y are:" +x +y);
        x = x + y;
        y = x - y;
        x = x - y;

        System.out.println("After swapping x and y are:" +x +y);
        System.out.println("x = " + x);
        System.out.println("y = " + y);
    }
}
